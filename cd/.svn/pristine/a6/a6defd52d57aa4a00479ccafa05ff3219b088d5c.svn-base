package com.wk.cd.build.ea.bean;

import com.wk.cd.bean.ActionRootInputBean;

/**
 * Class Description: 单步执行构建应用部署输入接口
 * @author Xul
 */
public class ExeStepBuildDeployInputBean extends ActionRootInputBean{

	private static final long serialVersionUID = 612855265915648328L;
	
	/**
	 * 任务编号
	 */
	private String work_id;

	public static final String WORK_IDCN = "任务编号";
	
	/**
	 * 阶段编号
	 */
	private int phase_id;

	public static final String PHASE_IDCN = "阶段编号";
	
	/**
	 * 跳过标志
	 */
	private boolean skip_flag;
	
	public static final String SKIP_FLAGCN = "跳过标志";

	/**
	 * @return work_id 任务编号
	 */
	public String getWork_id() {
		return work_id;
	}

	/**
	 * @param work_id 任务编号
	 */
	public void setWork_id(String work_id) {
		this.work_id = work_id;
	}

	/**
	 * @return phase_id 阶段编号
	 */
	public int getPhase_id() {
		return phase_id;
	}

	/**
	 * @param phase_id 阶段编号
	 */
	public void setPhase_id(int phase_id) {
		this.phase_id = phase_id;
	}

	/**
	 * @return skip_flag 跳过标志
	 */
	public boolean isSkip_flag() {
		return skip_flag;
	}

	/**
	 * @param skip_flag 跳过标志
	 */
	public void setSkip_flag(boolean skip_flag) {
		this.skip_flag = skip_flag;
	}
}
