package com.wk.cd.work.wk.bean;

import com.wk.cd.bean.ActionRootInputBean;
import com.wk.cd.enu.DEAL_RES;

/**
 * Class Description:任务复核输入接口
 * @author tlw
 */
public class CheckPendWorkInputBean
		extends ActionRootInputBean {

	private static final long serialVersionUID = 3004166325679823177L;

	/**
	 * 待处理流水号
	 */
	private String pend_wk_seq;

	public static final String PEND_WK_SEQCN = "待处理流水号";

	/**
	 * 处理结果
	 */
	private DEAL_RES deal_res;

	public static final String DEAL_RESCN = "处理结果";

	/**
	 * 处理说明
	 */
	private String deal_bk_desc;

	public static final String DEAL_BK_DESCCN = "处理说明";

	/**
	 * @return pend_wk_seq 待处理流水号
	 */
	public String getPend_wk_seq() {
		return pend_wk_seq;
	}

	/**
	 * @param pend_wk_seq 待处理流水号
	 */
	public void setPend_wk_seq(String pend_wk_seq) {
		this.pend_wk_seq = pend_wk_seq;
	}

	/**
	 * @return deal_res 处理结果
	 */
	public DEAL_RES getDeal_res() {
		return deal_res;
	}

	/**
	 * @param deal_res 处理结果
	 */
	public void setDeal_res(DEAL_RES deal_res) {
		this.deal_res = deal_res;
	}

	/**
	 * @return deal_bk_desc 处理说明
	 */
	public String getDeal_bk_desc() {
		return deal_bk_desc;
	}

	/**
	 * @param deal_bk_desc 处理说明
	 */
	public void setDeal_bk_desc(String deal_bk_desc) {
		this.deal_bk_desc = deal_bk_desc;
	}

}
