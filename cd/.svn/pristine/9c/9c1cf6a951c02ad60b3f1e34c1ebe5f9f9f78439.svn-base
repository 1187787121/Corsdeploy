package com.wk.cd.common.bean;

/**
 * Class Description:接口中文转换类
 * @author lixl
 */
public class BFieldNameBean {
	private String fcname;

	private String fname;

	private String fval;

	private String ftype;

	/**
	 * @return String Field CNname
	 */
	public String getFcname(){
		return this.fcname;
	}

	/**
	 * @param fcname Field CNname
	 */
	public void setFcname(String fcname){
		this.fcname = fcname;
	}

	/**
	 * @return String Field name
	 */
	public String getFname(){
		return this.fname;
	}

	/**
	 * @param fname Field name
	 */
	public void setFname(String fname){
		this.fname = fname;
	}

	/**
	 * @return String Field value
	 */
	public String getFval(){
		return this.fval;
	}

	/**
	 * @param fval Field value
	 */
	public void setFval(String fval){
		this.fval = fval;
	}

	/**
	 * @return String Field type
	 */
	public String getFtype(){
		return this.ftype;
	}

	/**
	 * @param ftype Field type
	 */
	public void setFtype(String ftype){
		this.ftype = ftype;
	}

}

