package com.wk.cd.build.en.bean;

import java.io.Serializable;
import java.util.List;

import com.wk.cd.enu.SERVER_OS;

/**
 * Class Description: 服务器详细及相关信息接口
 * @author Xul
 */
public class ServerDetAndRelBean implements Serializable{

	private static final long serialVersionUID = 3212800376285824015L;
	
	/**
	 *服务器名称
	 */
	private String server_name;

	public static final String SERVER_NAMECN = "服务器名称";

	/**
	 *服务器简称
	 */
	private String server_cn_name;

	public static final String SERVER_CN_NAMECN = "服务器简称";

	/**
	 *服务器描述
	 */
	private String server_bk_desc;

	public static final String SERVER_BK_DESCCN = "服务器描述";

	/**
	 *服务器地址
	 */
	private String server_ip;

	public static final String SERVER_IPCN = "服务器地址";
	
	/**
	 *操作系统
	 */
	private SERVER_OS server_os;

	public static final String SERVER_OSCN = "操作系统";
	
	/**
	 * 服务器下系统环境列表
	 */
	private List<SysEnvBean> sys_env_list;
	
	public static final String SYS_ENV_LISTCN = "服务器下系统环境列表";
	
	/**
	 *@return server_name 服务器名称
	 */
	public String getServer_name() {
		return this.server_name;
	}

	/**
	 *@param server_name 服务器名称
	 */
	public void setServer_name(String server_name) {
		this.server_name = server_name;
	}

	/**
	 *@return server_cn_name 服务器简称
	 */
	public String getServer_cn_name() {
		return this.server_cn_name;
	}

	/**
	 *@param server_cn_name 服务器简称
	 */
	public void setServer_cn_name(String server_cn_name) {
		this.server_cn_name = server_cn_name;
	}

	/**
	 *@return server_bk_desc 服务器描述
	 */
	public String getServer_bk_desc() {
		return this.server_bk_desc;
	}

	/**
	 *@param server_bk_desc 服务器描述
	 */
	public void setServer_bk_desc(String server_bk_desc) {
		this.server_bk_desc = server_bk_desc;
	}

	/**
	 *@return server_ip 服务器地址
	 */
	public String getServer_ip() {
		return this.server_ip;
	}

	/**
	 *@param server_ip 服务器地址
	 */
	public void setServer_ip(String server_ip) {
		this.server_ip = server_ip;
	}
	
	/**
	 * @return server_os 操作系统
	 */
	public SERVER_OS getServer_os() {
		return server_os;
	}

	/**
	 * @param server_os 操作系统
	 */
	public void setServer_os(SERVER_OS server_os) {
		this.server_os = server_os;
	}

	/**
	 * @return sys_env_list 服务器下系统环境列表
	 */
	public List<SysEnvBean> getSys_env_list() {
		return sys_env_list;
	}

	/**
	 * @param sys_env_list 服务器下系统环境列表
	 */
	public void setSys_env_list(List<SysEnvBean> sys_env_list) {
		this.sys_env_list = sys_env_list;
	}
}
