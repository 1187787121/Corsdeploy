package com.wk.cd.build.en.bean;

import java.util.List;

import com.wk.cd.build.en.info.CeSystemCfgInfo;

/**
 * Class Description: 
 * @author chiss
 */
public class SystemPackageBean {
	/**
	 * 文件包名
	 */
	private String package_name;

	public static final String PACKAGE_NAMECN = "文件包名";
	
	/**
	 * 应用系统配置文件列表
	 */
	private List<CeSystemCfgInfo> file_list;
	
	public static final String FILE_LISTCN = "应用系统配置文件列表";

	/**
	 * @return package_name 文件包名
	 */
	public String getPackage_name() {
		return package_name;
	}

	/**
	 * @param package_name 文件包名
	 */
	public void setPackage_name(String package_name) {
		this.package_name = package_name;
	}

	/**
	 * @return file_list 应用系统配置文件列表
	 */
	public List<CeSystemCfgInfo> getFile_list() {
		return file_list;
	}

	/**
	 * @param file_list 应用系统配置文件列表
	 */
	public void setFile_list(List<CeSystemCfgInfo> file_list) {
		this.file_list = file_list;
	}
}
