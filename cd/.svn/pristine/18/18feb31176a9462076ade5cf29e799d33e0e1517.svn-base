package com.wk.cd.remote.bean;

import java.util.Map;

/**
 * Class Description:
 * Created by lixl on 16-7-22.
 */
public class CommBean {
    private String remote_name;

    private String protocol;

    private String request_type;

    private String response_type;

    private String comm_type;

    private String connection_mode;

    private String packet_type;

    private String remote_address;

    private String timeout;

    private Map<String, String> conf;

    /**
     * 获取远程服务名称(服务器唯一标识)
     * @return
     */
    public String getRemote_name() {
        return remote_name;
    }

    /**
     * 设置远程服务器名称(服务器唯一标识)，必须输入
     * @param remote_name
     */
    public void setRemote_name(String remote_name) {
        this.remote_name = remote_name;
    }

    /**
     * 获取协议类型(http:// | tcp://)
     * @return
     */
    public String getProtocol() {
        return protocol;
    }

    /**
     * 设置协议类型(http:// | tcp://)
     * @param protocol
     */
    public void setProtocol(String protocol) {
        this.protocol = protocol;
    }

    /**
     * 获取请求报文格式(json/vrouter)
     * @return
     */
    public String getRequest_type() {
        return request_type;
    }

    /**
     * 设置请求报文格式(json/vrouter)
     * @param request_type
     */
    public void setRequest_type(String request_type) {
        this.request_type = request_type;
    }

    /**
     * 获取响应报文格式(json/vrouter)
     * @return
     */
    public String getResponse_type() {
        return response_type;
    }

    /**
     * 设置响应报文格式(json/vrouter)
     * @param response_type
     */
    public void setResponse_type(String response_type) {
        this.response_type = response_type;
    }

    /**
     * 获取通讯类型(client)
     * @return
     */
    public String getComm_type() {
        return comm_type;
    }


    /**
     * 设置通讯类型(client)
     * @param comm_type
     */
    public void setComm_type(String comm_type) {
        this.comm_type = comm_type;
    }

    /**
     * 获取通讯模式
     * SYNC_SHORT_CONNECTION(同步短连接)
     * SYNC_LONG_CONNECTION(同步长连接)
     * ASYNC_SHORT_CONNECTION(异步短连接)
     * ASYNC_LONG_CONNECTION(异步长连接)
     * @return
     */
    public String getConnection_mode() {
        return connection_mode;
    }

    /**
     * 获取通讯模式
     * SYNC_SHORT_CONNECTION(同步短连接)
     * SYNC_LONG_CONNECTION(同步长连接)
     * ASYNC_SHORT_CONNECTION(异步短连接)
     * ASYNC_LONG_CONNECTION(异步长连接)
     * @param connection_mode
     */
    public void setConnection_mode(String connection_mode) {
        this.connection_mode = connection_mode;
    }

    /**
     * 获取读取数据包模式(length|expr_length|fixed_length(offset|adjust|max|string|encoding))
     * length(adjust=-4,encoding=utf-8,...)
     * @return
     */
    public String getPacket_type() {
        return packet_type;
    }

    /**
     * 设置读取数据包模式(length|expr_length|fixed_length(offset|adjust|max|string|encoding))
     * length(adjust=-4,encoding=utf-8,...)
     * @param packet_type
     */
    public void setPacket_type(String packet_type) {
        this.packet_type = packet_type;
    }

    /**
     * 获取远程IP地址和端口(IP:PORT)
     * @return
     */
    public String getRemote_address() {
        return remote_address;
    }

    /**
     * 设置远程IP地址和端口(IP:PORT), 必须输入
     * @param remote_address
     */
    public void setRemote_address(String remote_address) {
        this.remote_address = remote_address;
    }

    /**
     * 获取超时时间(ms)
     * @return
     */
    public String getTimeout() {
        return timeout;
    }

    /**
     * 设置超时时间(ms)
     * @param timeout
     */
    public void setTimeout(String timeout) {
        this.timeout = timeout;
    }

    /**
     * 获取其他配置
     * @return
     */
    public Map<String, String> getConf() {
        return conf;
    }

    /**
     * 设置其他配置
     * @param conf
     */
    public void setConf(Map<String, String> conf) {
        this.conf = conf;
    }
}
