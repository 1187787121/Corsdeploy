logger.com.wk.nio=A1
logger.com.wk.nio.type=INFO_UP

logger.com.wk.db=A1
logger.com.wk.db.type=INFO_UP

logger.com.wk.cd=A1
logger.com.wk.cd.type=ALL

logger.com.wk.threadpool=A1
