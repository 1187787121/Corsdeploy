logger.watch.interval = 30000
logger.simplelog.type=ALL

logger.rootLogger=A1
logger.rootLogger.type=INFO_UP

logger.com.wk.service=A1
logger.com.wk.service.type=INFO_UP

logger.com.wk.service.impl=A1
