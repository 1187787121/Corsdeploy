package com.wk.cd.build.ea.bean;

/**
 * Class Description: 
 * @author "Zhangj"
 */
public class FileDetailBean {
	/**
	 * 文件名称（包含目录名）
	 */
	String file_child_name ;
	
	/**
	 * 文件是否为目录
	 */
	boolean dir_flag;

	public FileDetailBean(String file_child_name, boolean dir_flag) {
		super();
		this.file_child_name = file_child_name;
		this.dir_flag = dir_flag;
	}

	public String getFile_child_name() {
		return this.file_child_name;
	}

	public void setFile_child_name(String file_child_name) {
		this.file_child_name = file_child_name;
	}

	public boolean isDir_flag() {
		return this.dir_flag;
	}

	public void setDir_flag(boolean dir_flag) {
		this.dir_flag = dir_flag;
	}
	
	

}
